{
  "env": {
    "es6": true
  },
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "requireConfigFile": false
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
